<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.example.lesson02.dao.UsedGoodsDAO">		<!-- dao 위치 -->
 	<select id="selectUsedGoodsList" resultType="com.example.lesson02.model.UsedGoods">		<!-- id = dao의 메소드명, resultType = model클래스의 위치 -->
 		SELECT
 			*
 		FROM
 			`used_goods`		<!-- Query문에 오타 있으면 ###으로 시작하는 에러 뜸 -->
 	</select>
 </mapper>